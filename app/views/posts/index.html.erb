<h1>Post Listing</h1>
<%= link_to "My Blog", :posts %>

<%= link_to 'New Post', new_post_path %>

<table>
	<tr>
		<th>Title</th>
		<th>Text</th>
	</tr>

	<% @posts.each do |post| %>
		<tr>
			<td><%= link_to post.title, post_path(post.id) %></td>
			<td><%= post.text %></td>
			<td><%= link_to 'Edit', edit_post_path(post) %></td>
		</tr>
	<% end %>
</table>